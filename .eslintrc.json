{
  // make the window incudes so like document not compline by eslint
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    // let the js file can use jsx synatx and make eslint happy
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0, { "ignore": ["children"] }],
    "no-multi-assign": "off"
  }
}
